#%Module -*- tcl -*-
#
# Generated by Puppet
#
# for Tcl script use only
set	prdctversion	1.0
set	prdct		sgpu
set	PRDCT		[ string toupper $prdct ]
proc ModulesHelp { } {

	global prdctversion PRDCT

	puts stderr "\tAdds $PRDCT $prdctversion to your environment variables,"
	puts stderr "\tThis includes libraries and binaries that come"
	puts stderr "\twith the $PRDCT package."

}


module-whatis	"adds $PRDCT to your environment variables"

prepend-path	LD_LIBRARY_PATH	    $env(HOME)/sgpu/lib

conflict sgpu
